<Window x:Class="DataAccessWpf.ReadExcelDemo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataAccessWpf"
        mc:Ignorable="d"
        Title="ReadExcelDemo" Height="300" Width="300">
    <Grid>
        <ListView Margin="5" Name="lstStudents">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="证件号码" DisplayMemberBinding="{Binding Path=ID}" />
                    <GridViewColumn Header="医保卡号+姓名" DisplayMemberBinding="{Binding Path=HealthCareNumber}" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
